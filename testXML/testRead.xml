<?xml version="1.0"?>
<arguments xmlns="http://www.w3schools.com"
		   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		   xsi:schemaLocation="http://www.w3schools.com testRead.xsd">
		   
    <argument type = "positional">
        <name>Argument1</name>
        <description>Argument 1 description</description>
        <type>INTEGER</type>
    </argument>
    
    <argument type = "named">
        <name>OptionalArgument1</name>
        <description>Optional Argument 1 description</description>
        <type>STRING</type>
        <default>Value1</default>
    </argument>      
    
    <argument type = "named">
        <name>OptionalFlag1</name>
        <description>Optional Flag 1 description</description>
        <type>BOOLEAN</type>
        <default>false</default>
    </argument> 
</arguments>